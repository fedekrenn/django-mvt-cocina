{
  "builds": [
    {
      "src": "proyecto-final/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "proyecto-final/wsgi.py"
    }
  ]
}
